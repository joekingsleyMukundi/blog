<%- include('partials/header'); -%>
<h1>Compose</h1>
<form  action="/compose" method="post">
  <div class="form-group">
    <label for="exampleFormControlInput1">Title</label>
    <input type="text" name="postTitle" class="form-control" id="exampleFormControlInput1">
  </div>
  <div class="form-group">
      <label for="exampleFormControlTextarea1">Text</label>
      <textarea class="form-control" name="postBody" id="exampleFormControlTextarea1" rows="3"></textarea>
    </div>
  <button class="btn btn-primary" type="submit" name="submit">Publish</button>
</form>
<%- include('partials/footer'); -%>
